<div style="margin:auto;width:100%;border-radius:15px;border:1px solid #808080;padding:70px;">

    <div class="row">
        <label for="Name" class="col-md-1 control-label">Code:</label>
        <div class="form-group col-md-2" style="margin-left:05px">
            <input type="text" class="form-control" id="Name" />
        </div>

        <label for="Name" class="col-md-1 control-label">Start Date:</label>
        <div class="form-group col-md-3" style="margin-left:0px">
            <input type="date" class="form-control" id="Name" />
        </div>
        <div class="form-group col-md-2" style="margin-left:05px">
            <input type="checkbox" name="vehicle1" value="Bike">  Active<br>
        </div>
        <div class="form-group col-md-2" style="margin-left:05px">
            <input type="checkbox" name="vehicle1" value="Bike"> Primary Practice<br>
        </div>
    </div>
    <div class="row">
        <label for="Name" class="col-md-1 control-label">Name:</label>
        <div class="form-group col-md-5" style="margin-left:05px">
            <input type="text" class="form-control" id="Name" />
        </div>
    </div>
    <div class="row">
        <label for="Name" class="col-md-1 control-label">Type:</label>
        <div class="form-group col-md-3" style="margin-left:05px">
            <input type="text" class="form-control" id="Name" />
        </div>

        <label for="Name" class="col-md-2 control-label">Facility Type:</label>
        <div class="form-group col-md-3" style="margin-left:05px">
            <input type="text" class="form-control" id="Name" />
        </div>
    </div>
   
   

    <div class="container">
